<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Alpha_Project</servlet-name>
		<servlet-class>project.alpha.Alpha_ProjectServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Alpha_Project</servlet-name>
		<url-pattern>/alpha_project</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Patient_Details</servlet-name>
		<servlet-class>project.alpha.PatDetailsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Patient_Details</servlet-name>
		<url-pattern>/personal_details.do</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<!-- <welcome-file>patient_form/patient_info.html</welcome-file> -->
	</welcome-file-list>
</web-app>
