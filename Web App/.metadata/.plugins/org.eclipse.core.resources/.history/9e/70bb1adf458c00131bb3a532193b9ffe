package webrtc.eval.model;

import java.util.ArrayList;

public class Client {
	String cName, cID;
	Clinic cClinic;
	ArrayList<String> cPatientIDs;
	
	public Client(String name, Clinic clinic, String id){
		this.cName = name;
		this.cClinic = clinic;
		this.cID = id;
		
		cPatientIDs = new ArrayList<String>();
	}
}
