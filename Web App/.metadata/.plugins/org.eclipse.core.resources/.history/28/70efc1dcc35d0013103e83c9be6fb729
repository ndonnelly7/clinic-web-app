package project.beta.model;

import javax.persistence.Entity;

@Entity
public class DrugHistory {
	String drug;
	String time;
	String notes;
	
	public DrugHistory(String drug, String time, String notes)
	{
		this.drug = drug;
		this.time = time;
		this.notes = notes;
	}
	
	@Override
	public String toString() {
		return "MedHisotry [condition=" + drug
				+ ", time=" + time
				+ ", notes" + notes
				+ "]";
	}
}
