package webrtc.eval.model;

import java.util.ArrayList;

public class Client {
	String cName, cID;
	Clinic cClinic;
	ArrayList<String> cPatientIDs;
	
	public Client(String name, Clinic clinic, String id){
		this.cName = name;
		this.cClinic = clinic;
		this.cID = id;
		
		cPatientIDs = new ArrayList<String>();
	}

	public String getcName() {
		return cName;
	}

	public void setcName(String cName) {
		this.cName = cName;
	}

	public String getcID() {
		return cID;
	}

	public void setcID(String cID) {
		this.cID = cID;
	}

	public Clinic getcClinic() {
		return cClinic;
	}

	public ArrayList<String> getcPatientIDs() {
		return cPatientIDs;
	}

	public void setcPatientIDs(ArrayList<String> cPatientIDs) {
		this.cPatientIDs = cPatientIDs;
	}
	
	
}
