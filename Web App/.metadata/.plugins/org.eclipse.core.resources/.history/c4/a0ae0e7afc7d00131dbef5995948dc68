package peerjs;

public class MExUser {

	String name, peerID;
	
	public MExUser(String name, String peerID) {
		name = this.name;
		peerID = this.peerID;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getPeerID() {
		return peerID;
	}

	public void setPeerID(String peerID) {
		this.peerID = peerID;
	}
}
