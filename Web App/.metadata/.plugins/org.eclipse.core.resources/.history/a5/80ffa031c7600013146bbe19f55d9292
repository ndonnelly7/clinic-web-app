package project.beta.model;

public class Activity {
	String type, involvement, time_changed, notes;
	int current_hours, prev_hours;
	
	public Activity(String type, String involvement){
		this.type= type;
		this.involvement = involvement;
		
		time_changed = "";
		notes = "";
		current_hours = -1;
		prev_hours = -1;
	}
}
